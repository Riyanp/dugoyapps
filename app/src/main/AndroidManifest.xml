<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dugoy.dugoyapps">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_dugoy_apps"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_dugoy_apps"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.dugoy.dugoyapps.ListFeatureActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.dugoy.dugoyapps.CRUDActivity" />
        <activity android:name="com.example.dugoy.dugoyapps.MyRecycleActivity"></activity>
        <activity android:name="com.example.dugoy.dugoyapps.service.RetrofitActivity" />
        <activity android:name="com.example.dugoy.dugoyapps.MartabakActivity" />
        <activity android:name="com.example.dugoy.dugoyapps.MartabakDetailActivity" />
    </application>

</manifest>
